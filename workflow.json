      "parameters": {
        "jsCode": "const command = $node[\"Parse Command\"].json.command;\nlet message = '';\n\nif (command === 'LIST') {\n  \n  let files = $items(\"Search files and folders1\").map(item => item.json);\n\n  if (files.length === 0) {\n    message = 'üìÅ The folder is empty.';\n  } else {\n    message = 'üìÅ Here are the files in your folder:\\n';\n    for (const file of files) {\n      message += `‚Ä¢ ${file.name || 'Unnamed File'}\\n`;\n    }\n  }\n\n} else if (command === 'DELETE') {\n  message = 'üóëÔ∏è File deleted successfully.';\n\n} else if (command === 'MOVE') {\n  message = '‚úÖ File moved successfully.';\n\n} else if (command === 'UPLOAD') {\n  \n  message = '‚úÖ File uploaded successfully.';\n\n} else if (command === 'SUMMARY') {\n  \n  if (!items || items.length === 0) {\n    message = 'üìù No summaries available.';\n  } else {\n    message = 'üìù Document Summaries:\\n';\n    for (const item of items) {\n      message += `${item.json.output}\\n`;\n    }\n  }\n\n} else {\n  message = '‚ö†Ô∏è Unknown command.';\n}\n\nreturn {\n  json: { reply: message }\n};\n"
      },
